{
  "tabWidth": 2,
  "useTabs": false,
  "plugins": [
    "@pandabox/prettier-plugin",
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^solid",
    "<THIRD_PARTY_MODULES>",
    "^@revolt",
    "^@material-design-icons|^solid-icons",
    "^\\.\\.",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}